<body>
	<div class="header ">
		<div class="container">
			<div class="logo">
				<a href="<%= url_for root_path %>"><img src="../logo.png" alt=""  /></a>  
			</div>
			<nav id="main_menu">
				<div class="menu_wrap">
					<ul class="nav sf-menu">
						<li class="sub-menu active"><a href="<%= url_for root_path %>">Home</a></li>
						<li class="sub-menu"><%= link_to "Leagues", leagues_path %></li>
						<% if user_signed_in? %>
							<li><%= link_to "My Teams", teams_path %> </li>	
							<li class="submenu">
								<%= link_to "Sign out", destroy_user_session_path, method: :delete%>
							</li>
						<% else %>
							<li class="submenu">
								<%= link_to "Sign in", new_user_session_path%>
							</li>
							<li class="submenu"><%= link_to "Sign up", new_user_registration_path %></li>
						<% end %>
					</ul>
				</div>
			</nav>
		</div>
	</div>
	<!--//header-->
	<!--banner-->

	<div class="container wrapper">
		<div class="inner_content">
			<%= bootstrap_flash %>
			<%= yield %>
		</div>
	</div>
	<!--//page-->
